{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ]
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "all dependencies",
      "automerge": true
    }
  ],
  "gradle": {
    "enabled": true,
    "fileMatch": ["gradle/libs.versions.toml"]
  }
}
